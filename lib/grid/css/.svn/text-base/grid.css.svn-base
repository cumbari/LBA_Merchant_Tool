/*- Menu Tabs 6--------------------------- */

    #tabs6 { float:left; width:100%; font-size:100%; line-height:normal; font-weight:bold; font-weight:normal; border-bottom:0px solid #666; }
#tabs6 ul { margin:0px 0px 5px 0px; padding:0px 0px 0 0px; list-style:none; }
#tabs6 li { display:inline; margin:0; padding:0; }
#tabs6 a { float:left; margin:0; padding:0 0 0 6px; text-decoration:none; background-image:  url(../images/tableft6.gif); background-repeat: no-repeat; background-position: left top; }
#tabs6 a span { float:left; display:block; padding:5px 15px 4px 6px; color:#881d0a; background-image:  url(../images/tabright6.gif); background-repeat: no-repeat; background-position: right top; }
/* Commented Backslash Hack hides rule from IE5-Mac \*/
    #tabs6 a span { float:none; }
/* End IE5-Mac hack */
    #tabs6 a:hover span { color:#FFF; }
#tabs6 a:hover { background-position:0% -42px; }
#tabs6 a:hover span { background-position:100% -42px; }
#tabs6 #current a { background-position:0% -42px; }
#tabs6 #current a span { background-position:100% -42px; color:#fff; }
/******** Left Navigation ********/

#left-navigation { color:#018dc0; width:190px; }
#left-navigation ul { margin:0px; padding:10px 0px 0px 0px; list-style-type:none; }
#left-navigation ul li { margin:0px; padding:0px; line-height:25px; border-bottom:1px solid #E1E1E1; display:block; }
#left-navigation ul li a { padding:0px 0px 1px 15px; display:block; text-decoration:none; }
#left-navigation ul li a:hover { color:#018dc0; padding:0px 0px 0px 15px; text-decoration:none; background-color:#ffffff; border-bottom:1px solid #9f3922; cursor:pointer; }
#left-navigation #active a { color: #fff; text-decoration:none; padding:0px 0px 0px 15px; background-color:#c95137; border-bottom:1px solid #9f3922; }
